{
  "expo": {
    "name": "GEOMAP",
    "slug": "geomap",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/icons/geomap-icon-1.png",
    "scheme": "geomap",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/geomap-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/geomap-icon-1.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.geomap",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBaFBUQbNHfb-hsrf5DiIVi9G_Lxf-r_Js"
        }
      },
      "permissions": ["ACCESS_FINE_LOCATION", "ACCESS_COARSE_LOCATION", "CAMERA", "READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker", {
          "photoPermission": "Allow GEOMAP to access your photos"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow GEOMAP to access your camera",
          "microphonePermission": "Allow GEOMAP to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
